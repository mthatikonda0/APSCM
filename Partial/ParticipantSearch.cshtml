@model AGE.CMS.Data.Models.Case.viewParticipantSearch

<div class="well container " id="ParticipantSearchScreen">
    <div class="title">
        <h3>
            Participant Search Results
        </h3>
    </div>
    <div class="modal-body">
        <div class="well" id="eCCPIS" style="background-color:#fff">
            <h3>eCCPIS Records</h3>
            @if (Model.eCCPIS.IsFound != false)
            {
                <table class="table table-striped table-hover " border="1">
                    <caption style="display: none">eCCPIS Client Records</caption>
                    <thead>
                        <tr>
                            <th class="col-md-1">Client Id</th>
                            <th class="col-md-1">Last Name</th>
                            <th class="col-md-1">First Name</th>
                            <th class="col-md-1">M. I.</th>
                            <th class="col-md-1">Birth Date</th>
                            <th class="col-md-1">Sex</th>
                            @*  <th class="col-md-1">Race</th>*@
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="">
                            <td>@Html.DisplayFor(model => model.eCCPIS.ClientID)</td>
                            <td>@Html.DisplayFor(model => model.eCCPIS.LastName)</td>
                            <td>@Html.DisplayFor(model => model.eCCPIS.FirstName)</td>
                            <td>@Html.DisplayFor(model => model.eCCPIS.MiddleInitial)</td>
                            <td>@Html.DisplayFor(model => model.eCCPIS.DoB)</td>
                            <td>@Html.DisplayFor(model => model.eCCPIS.Sex)</td>
                            @* <td>@Html.DisplayFor(model => model.Race)</td>*@
                        </tr>

                    </tbody>
                </table>
                if (Model.eCCPIS.ClientServices.Count > 0)
                {
                    <b>Client CCP Services</b>
                    <br />
                    foreach (var service in Model.eCCPIS.ClientServices)
                    {
                        <p style="border:groove">
                            @Html.LabelFor(m => service.ServiceCode): @Html.DisplayFor(m => service.ServiceCode) <br /><br />

                            @Html.LabelFor(m => service.Action): @Html.DisplayFor(m => service.Action)&nbsp;&nbsp;

                            @Html.LabelFor(m => service.ServMaxUnits): @Html.DisplayFor(m => service.ServMaxUnits)&nbsp;&nbsp;
                            @Html.LabelFor(m => service.StartDt):@service.StartDt &nbsp;&nbsp;
                            @Html.LabelFor(m => service.EndDt):@service.EndDt <br />
                            @Html.LabelFor(m => service.CCUContNum): @Html.DisplayFor(m => service.CCUContNum)&nbsp;&nbsp;
                            @Html.LabelFor(m => service.ProvContNum): @Html.DisplayFor(m => service.ProvContNum)&nbsp;&nbsp;
                        </p>





                    }
                }

            }
            else
            {
                <div class="alert alert-warning" role="alert">
                    No Record Found !
                </div>
            }
        </div>
        <div class="well" id="MCOInformation" style="background-color:#fff">
            <h3>MCO Information</h3>
            @if (Model.ListMCOInfo != null && Model.ListMCOInfo.Any())
            {
                <table class="table table-striped table-hover " border="1">
                    <caption style="display: none">MCO Information</caption>
                    <thead>
                        <tr>
                            <th class="col-md-1">MCO Name</th>
                            <th class="col-md-1">Begin Date</th>
                            <th class="col-md-1">End Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.ListMCOInfo)
                        {
                            <tr class="">
                                <td>@Html.DisplayFor(modelItem => item.MCOName)</td>
                                <td>@Html.DisplayFor(modelItem => item.BeginDate)</td>
                                <td>@Html.DisplayFor(modelItem => item.EndDate)</td>
                            </tr>
                        }
                    </tbody>
                </table>
            }
            else
            {
                <div class="alert alert-warning" role="alert">
                    No Record Found !
                </div>
            }
        </div>
        <div class="well" id="HFSRecipient" style="background-color:#fff">
            <h3>HFS Recipient Records</h3>
            @if (Model.HFSRecipient.IsFound != false)
            {
                <table class="table table-striped table-hover " border="1">
                    <caption style="display: none">HFS Recipient Records</caption>
                    <thead>
                        <tr>
                            <th class="col-md-1">RIN</th>
                            <th class="col-md-1">Last Name</th>
                            <th class="col-md-1">First Name</th>
                            <th class="col-md-1">Birth Date</th>
                            <th class="col-md-1">Sex</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="">
                            <td>@Html.DisplayFor(model => model.HFSRecipient.ID_NUM)</td>
                            <td>@Html.DisplayFor(model => model.HFSRecipient.LAST_NAME)</td>
                            <td>@Html.DisplayFor(model => model.HFSRecipient.FST_NAME)</td>
                            <td>@Html.DisplayFor(model => model.HFSRecipient.BRTH_DTE)</td>
                            <td>@Html.DisplayFor(model => model.HFSRecipient.SEX_CD)</td>
                        </tr>
                    </tbody>
                </table>
            }
            else
            {
                <div class="alert alert-warning" role="alert">
                    No Record Found !
                </div>
            }
        </div>
        <div class="well" id="OBRAWaiver" style="background-color:#fff">
            <h3>OBRA Waivers</h3>
            @if (Model.OBRAWaiver.IsFound != false)
            {
                <table class="table table-striped table-hover " border="1">
                    <caption style="display: none">OBRA Waivers</caption>
                    <thead>
                        <tr>
                            <th class="col-md-1">RIN</th>
                            <th class="col-md-1">MasterFile Name</th>
                            <th class="col-md-1">Birth Date</th>
                            <th class="col-md-1">Waiver</th>
                            <th class="col-md-1">Begin Date</th>
                            <th class="col-md-1">End Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="">
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.RECIP_NUMBER)</td>
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.MASTERFILE_NAME)</td>
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.BIRTH_DATE)</td>
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.WAIVER)</td>
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.BEGIN_DATE)</td>
                            <td>@Html.DisplayFor(model => model.OBRAWaiver.END_DATE)</td>
                        </tr>
                    </tbody>
                </table>
            }
            else
            {
                <div class="alert alert-warning" role="alert">
                    No Record Found !
                </div>
            }
        </div>
        <div class="well" id="DeathRecords" style="background-color:#fff">
            <h3>Death Records</h3>
            @if (Model.DeathRecord.IsFound != false)
            {
                <table class="table table-striped table-hover " border="1">
                    <caption style="display: none">Death Records</caption>
                    <thead>
                        <tr>
                            <th class="col-md-1">Name</th>
                            <th class="col-md-1">Sex</th>
                            <th class="col-md-1">Birth Date</th>
                            <th class="col-md-1">Death Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="">
                            <td>@Html.DisplayFor(model => model.DeathRecord.FullName)</td>
                            <td>@Html.DisplayFor(model => model.DeathRecord.Sex)</td>
                            <td>@Html.DisplayFor(model => model.DeathRecord.BirthDate)</td>
                            <td>@Html.DisplayFor(model => model.DeathRecord.DeathDate)</td>
                        </tr>
                    </tbody>
                </table>
            }
            else
            {
                <div class="alert alert-warning" role="alert">
                    No Record Found !
                </div>
            }
        </div>
    </div>
</div>
