                                                                                    <a rel="tooltip" class=" btn btn-success btn-outline" title="Create New Law Enforcement " href='@Url.Action("LawEnforcement", "Case", new { Id = 0, IntakeId = item.Id })'><span class="glyphicon glyphicon-plus"></span><span class="textspace"></span>Law Enforcement</a>
                                                                                    <br />
                                                                                    <div class="row nextline"></div>
                                                                                    @{ var approvemessage = ""; }
                                                                                    @if (item.ListofLawEnforcement != null && item.ListofLawEnforcement.Any())
                                                                                    {
                                                                                        foreach (var lawenfo in item.ListofLawEnforcement)
                                                                                        {
                                                                                            if (lawenfo.Id > 0)
                                                                                            {
                                                                                                if (lawenfo.PartOneDone == true && lawenfo.PartTwoDone == true)
                                                                                                {
                                                                                                    <a rel="tooltip" class="btn btn-default btn-sm" title="View Law Enforcement " href='@Url.Action("ViewLawEnforcement", "Case", new { Id = lawenfo.Id, IntakeId = item.Id })'><span class="glyphicon glyphicon-eye-open"></span></a>

                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <a rel="tooltip" class=" btn btn-default btn-sm" title="Edit Law Enforcement " href='@Url.Action("LawEnforcement", "Case", new { Id = lawenfo.Id, IntakeId = item.Id })'><span class="glyphicon glyphicon-edit"></span></a>
                                                                                                    <a rel="tooltip" class="btn btn-default btn-sm" title="View Law Enforcement " href='@Url.Action("ViewLawEnforcement", "Case", new { Id = lawenfo.Id, IntakeId = item.Id })'><span class="glyphicon glyphicon-eye-open"></span></a>
                                                                                                }

                                                                                                if (lawenfo.PartOneDone == null && lawenfo.PartTwoDone == null)
                                                                                                {

                                                                                                    approvemessage = "Part 1 is Incomplete and Part 2 is Incomplete";

                                                                                                }

                                                                                                else if (lawenfo.PartOneDone == false && lawenfo.PartTwoDone == null)
                                                                                                {

                                                                                                    approvemessage = "Part 1 is Submited and Part 2 Incomplete";

                                                                                                }
                                                                                                else if (lawenfo.PartOneDone == true && lawenfo.PartTwoDone == null)
                                                                                                {
                                                                                                    approvemessage = "Part 1 is Approved and Part 2 is Incomplete";
                                                                                                }

                                                                                                else if (lawenfo.PartTwoDone == false && lawenfo.PartOneDone == true)
                                                                                                {

                                                                                                    approvemessage = "Part 2 is Submited part 1 is Approved";

                                                                                                }
                                                                                                else if (lawenfo.PartTwoDone == true && lawenfo.PartOneDone == true)
                                                                                                {
                                                                                                    approvemessage = "Completed";
                                                                                                }


                                                                                                <span class="textspace"></span><b>Status  : @approvemessage  &nbsp; Date Created: @lawenfo.DateCreated.Value.ToShortDateString() </b> <br />
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <div class="Suspiciousdeath col-lg-6 zommable">
                                                                    <div class="panel-group">
                                                                        <div class="panel panel-default">
                                                                            <div class="panel-heading subheadings">
                                                                                <div class="">
                                                                                    <div class="btn-group">
                                                                                        <b style="color: #fff" rel="tooltip" data-toggle="collapse" data-target="" href="">
                                                                                            Suspicious Death
                                                                                        </b>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div>
                                                                                <div class="panel-body">
                                                                                    @if (item.SuspiciousDeath.Id > 0)
                                                                                    {
                                                                                        <a rel="tooltip" class="btn btn-default btn-sm" title="Edit Suspicious Death" href='@Url.Action("SuspiciousDeath", "Case", new { Id = item.SuspiciousDeath.Id, IntakeId = item.Id })'><span class="glyphicon glyphicon-edit"></span></a>
                                                                                        <a rel="tooltip" class=" btn btn-default btn-sm" title="View Suspicious Death" href='@Url.Action("ViewSuspiciousDeath", "Case", new { Id = item.SuspiciousDeath.Id, IntakeId = item.Id })'><span class="glyphicon glyphicon-eye-open"></span></a>


                                                                                        if (item.SuspiciousDeath.PartOneDone == null && item.SuspiciousDeath.PartTwoDone == null)
                                                                                        {

                                                                                            approvemessage = "Part 1 is Incomplete and Part 2 is Incomplete";

                                                                                        }

                                                                                        else if (item.SuspiciousDeath.PartOneDone == false && item.SuspiciousDeath.PartTwoDone == null)
                                                                                        {

                                                                                            approvemessage = "Part 1 is Submited and Part 2 Incomplete";

                                                                                        }
                                                                                        else if (item.SuspiciousDeath.PartOneDone == true && item.SuspiciousDeath.PartTwoDone == null)
                                                                                        {
                                                                                            approvemessage = "Part 1 is Approved and Part 2 is Incomplete";
                                                                                        }

                                                                                        else if (item.SuspiciousDeath.PartTwoDone == false && item.SuspiciousDeath.PartOneDone == true)
                                                                                        {

                                                                                            approvemessage = "Part 2 is Submited part 1 is Approved";

                                                                                        }
                                                                                        else if (item.SuspiciousDeath.PartTwoDone == true && item.SuspiciousDeath.PartOneDone == true)
                                                                                        {
                                                                                            approvemessage = "Completed";
                                                                                        }


                                                                                        <span class="textspace"></span><b>Status : @approvemessage &nbsp; Date Created: @item.SuspiciousDeath.DateCreated.Value.ToShortDateString() </b>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <a rel="tooltip" class=" btn btn-success btn-outline" title="Create Suspicious Death" href='@Url.Action("SuspiciousDeath", "Case", new { Id = 0, IntakeId = item.Id })'><span class="glyphicon glyphicon-plus"></span><span class="textspace"></span>Suspicious Death</a>
                                                                                        <div class="row nextline"></div>
                                                                                    }




                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    }
                                }

                                else
                                {
                                    <p>There is no SIR yet</p>
                                }

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel-group" id="accordionRIR">
            <div class="panel panel-default" id="panelRIR">
                <div class="panel-heading intakeheadings " data-toggle="collapse" data-target="#collapseRIR" href="#collapseRIR">
                    <w style="text-align: center"><h4 style="color:#8b2433">Related Intake Report(s)</h4></w>

                </div>
                <div id="collapseRIR" class="panel-collapse collapse">
                    <div class="panel-body">
                        <div id="RIR" class="col-lg-12">
                            <div class="  ">
                                @if (Model.ListofIntakeForms != null && Model.ListofIntakeForms.Any())
                                {
                                    foreach (var item in Model.ListofIntakeForms)
                                    {
                                        if (item.IntakeReportTypeId == 3)
                                        {

                                            <div class="panel-heading" style="border: 1px solid #337ab7; padding-top: 3px; padding-bottom: 3px">
                                                <div class="">
                                                    <div class="btn-group">
                                                        <a rel="tooltip" class="btn btn-default btn-sm" title="View Intake" href='@Url.Action("ViewIntake", "Intake", new { ClientId = Model.Id, Id = item.Id })'><span class="glyphicon glyphicon-eye-open"></span></a>
                                                    </div>
                                                    <b style="color: black">Intake Id: @item.Id &nbsp; Intake Date: @item.DateIntake.Value.ToShortDateString() </b>
                                                    @if (User.IsInRole("CMS_Admin") || User.IsInRole("CMS_IDOAStaff"))
                                                    {
                                                        <button type="button" class="btn btn-primary btn-sm updateIntakeType pull-right" data-transfer="@item.Id"><span class="glyphicon glyphicon-circle-arrow-up"></span><span class="textspace"></span>Update </button>
                                                        <div class="pull-right"> @Html.DropDownListFor(modelItem => item.IntakeReportTypeId, new SelectList(new[] { new AGE.CMS.Data.Models.LookupModels.viewIntakeReportType { Id = 0, Description = " Select Intake Type" } }.Union(Model.caselookup.ListOfIntakeReportTypes), "Id", "Description", item.IntakeReportTypeId), new { @class = "form-control " })</div>
                                                        @Html.HiddenFor(model => item.Id)

                                                    }


                                                </div>
                                            </div>
                                            <br />
                                        }
                                    }

                                }
                                else
                                {
                                    <p>There is no RIR yet</p>
                                }

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
